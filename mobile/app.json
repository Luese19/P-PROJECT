{
  "expo": {
    "name": "Jeep-Track",
    "slug": "jeep-track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/JEEP.png",
    "scheme": "jeep-track",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/JEEP.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a73e8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeeptrack.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Jeep-Track needs your location to show nearby jeepneys and calculate ETAs.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Jeep-Track needs your location to share your position as a driver.",
        "UIBackgroundModes": [
          "location"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/JEEP.png",
        "backgroundColor": "#1a73e8"
      },
      "package": "com.jeeptrack.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/JEEP.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Jeep-Track to use your location for tracking."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "68cdd152-881d-47f1-953e-f508f63fb581"
      }
    },
    "owner": "luesedev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/68cdd152-881d-47f1-953e-f508f63fb581"
    }
  }
}
